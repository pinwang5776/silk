#: main.go:33
msgid "[Error] both input file and output file are required.\n"
msgstr "[错误] 输入文件和输入都是必填的。\n"

#: main.go:39
msgid "failed to open input file %q: %+v"
msgstr "打开输入文件 %q 失败: %+v"

#: main.go:44
msgid "failed to decode input file %q: %+v"
msgstr "对输入文件 %s 解码失败"

#: main.go:49
msgid "failed to open output file %q: %+v"
msgstr "打开输入文件 %q 失败: %+v"

#: main.go:54
msgid "failed to write output file %q: %+v"
msgstr "写入输出文件 %q 失败: %+v"

#: main.go:61
msgid "Silk decoder, Go version, based on v1.0.9 of C version"
msgstr "Silk 解码器，Go 语言版本，基于 v1.0.9 的 C 语言版本"

#: main.go:62
msgid "Decode silk v3 file to pcm, by youthlin"
msgstr "将 silk v3 格式的文件解码为 pcm, 作者：youthlin"

#: main.go:63
msgid "GitHub: https://github.comyouthlin/silk"
msgstr "GitHub: https://github.comyouthlin/silk"

#: main.go:65
#, c-format
msgid ""
"Usage: %s -i <input file> -o <output file> [-sampleRate <hz>] [-l <language>]"
msgstr "用法：%s -i <输入文件> -o <输出文件> [-sampleRate <采样率>] [-l <语言>]"

#: main.go:66
msgid "  -i <input file>\tBitstream input to decoder"
msgstr "  -i <输入文件>\t\t要解码的输入文件，silk 格式"

#: main.go:67
msgid "  -o <output file>\tSpeech output from decoder"
msgstr "  -o <输出文件>\t\t解码后的 pcm 语音文件"

#: main.go:68
msgid "  -sampleRate <hz>\tSample rate in Hz, default 24000"
msgstr "  -sampleRate <采样率>\t单位为赫兹，默认值为 24000"

#: main.go:69
msgid "  -l <language>\t\tLanguage path(pointer to po file/dir)"
msgstr "  -l <语言>\t\t指定语言路径(po 文件或文件夹)"
